{
  "name": "router-cnc-nodejs-arduino",
  "version": "0.1.1",
  "description": "cnc realizado con nodejs electron serialport arduino.",
  "dependencies": {},
  "devDependencies": {
    "electron-builder": "7.15.2",
    "electron-packager": "8.2.0",
    "electron-prebuilt": "1.4.5",
    "electron-rebuild": "~1.3.0",
    "grunt": "~1.0.1",
    "grunt-contrib-pug": "~1.0.0",
    "grunt-contrib-jshint": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-docco-plus": "~3.0.0",
    "grunt-mocha-test": "0.13.2",
    "grunt-shell": "~2.0.0",
    "pug": "2.0.0-beta6",
    "jshint": "~2.9.4",
    "jshint-stylish": "~2.2.1",
    "load-grunt-tasks": "~3.5.2",
    "mocha": "~3.1.2"
  },
  "scripts": {
    "postinstall": "cd ./app/ ; npm install",
    "rebuild": "grunt buildmodule",
    "start": "grunt run",
    "test": "mocha ./tests/**/*.js"
  },
  "author": {
    "name": "Marani Matias Ezequiel",
    "email": "maranimatias@gmail.com"
  },
  "engines": {
    "node": "~6.9.1",
    "npm": "~3.10.9"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaraniMatias/router-cnc-nodejs-arduino"
  },
  "keywords": [
    "cnc",
    "arduino",
    "electron",
    "express",
    "nodejs",
    "serialport",
    "gcode"
